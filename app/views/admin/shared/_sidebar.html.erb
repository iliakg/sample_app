<div class='profile_item'>
  <div class="email"><%= current_admin.email %></div>
  <a href='<%= destroy_admin_session_path %>' data-method="delete"><i class="material-icons">exit_to_app</i></a>
</div>

<ul class="sidebar-list">
  <li <%= dashboard_active %>>
    <a href='<%= admin_path %>'><i class="material-icons">home</i><%= tt(:dashboard) %></a>
  </li>
  <li <%= users_active %>>
    <a href='<%= admin_users_path %>'><i class="material-icons">people</i><%= tt(:users, :title) %></a>
  </li>
  <li <%= categories_active %>>
    <a href='<%= admin_categories_path %>'><i class="material-icons">list</i><%= tt(:categories, :title) %></a>
  </li>
  <li <%= files_active %>>
    <a href='<%= admin_file_items_path %>'><i class="material-icons">insert_drive_file</i><%= tt(:file_items, :title) %></a>
  </li>
  <li <%= posts_active %>>
    <a href='<%= admin_posts_path %>'><i class="material-icons">subject</i><%= tt(:posts, :title) %></a>
  </li>
  <li <%= pages_active %>>
    <a href='<%= admin_pages_path %>'><i class="material-icons">description</i><%= tt(:pages, :title) %></a>
  </li>
</ul>
